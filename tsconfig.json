{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/constants/*"],
      "@assets/*": ["./src/assets/*"]
    },
    "typeRoots": ["./node_modules/@types", "src/types"]
  }
}
